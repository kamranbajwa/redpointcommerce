<% content_for :page_title do %>
  <%= Spree.t(:editing_prototype) %>
<% end %>

<% content_for :page_actions do %>
  <li>
    <%= link_to spree.admin_prototypes_path, class: "btn blue" do %><i class="fa fa-arrow-left"></i> <%= Spree.t(:back_to_prototypes_list)%><% end %>
  </li>
<% end %>
 <div class="portlet box blue">
   <div class="portlet-title">
    <div class="caption">
       Update Prototype
    </div>
    <div class="tools">
      <a href="" class="collapse" data-original-title="" title="">
      </a>
    </div>
  </div>
  <div class="portlet-body form">
<%= render :partial => 'spree/shared/error_messages', :locals => { :target => @prototype } %>

<%= form_for [:admin, @prototype] do |f| %>
  <div class="form-body">
    <%= render :partial => 'form', :locals => { :f => f } %>
    <div class="clear"></div>
  </div>

    <%= render :partial => 'spree/admin/shared/edit_resource_links' %>
<% end %>
</div>
</div>
