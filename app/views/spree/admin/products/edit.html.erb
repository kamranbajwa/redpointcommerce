<% content_for :page_actions do %>
  <li class="btn-padding"><%= link_to session[:return_to] || admin_products_url, class: "btn blue" do %><i class="fa fa-arrow-left"></i><%= Spree.t(:back_to_products_list)%> <% end %></li>
  <% if can?(:create, Spree::Product) %>
    <li id="new_product_link">
      <%= link_to new_object_url, { class: "btn blue", :id => 'admin_new_product' } do %><i class="fa fa-plus"></i><%= Spree.t(:new_product)%><% end %>
    </li>
  <% end %>
<% end %>


 <div class="portlet box blue col-md-9">
   <div class="portlet-title">
    <div class="caption">
       <%= Spree.t(:new_product) %>
    </div>
    <div class="tools">
      <a href="" class="collapse" data-original-title="" title="">
      </a>
    </div>
  </div>
  <div class="portlet-body form">
<%= render :partial => 'spree/shared/error_messages', :locals => { :target => @product } %>

<%= form_for [:admin, @product], :method => :put, :html => { :multipart => true } do |f| %>
  <div class="form-body">
    <%= render :partial => 'form', :locals => { :f => f } %>
  </div>
    <%= render :partial => 'spree/admin/shared/edit_resource_links' %>
<% end %>
</div>
</div>
<div class="col-md-3">
  <%= render :partial => 'spree/admin/shared/product_tabs', :locals => { :current => 'Product Details' } %>
</div>