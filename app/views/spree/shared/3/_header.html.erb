<link rel="stylesheet" href="/assets/<%= @selected_template.template_no%>/background.css" />
<link rel="stylesheet" href="/assets/<%= @selected_template.template_no%>/slider.css" />
<!-- HEADER -->

<header class="front-header" style="height:129px !important;" >
  <div class="menu-open">
    <div class="fa fa-bars"></div>
  </div>
  <!-- end menu-open -->
  
  <div class="mobile-menu-wrap">
    <div class="fa fa-times menu-close"></div>
    <div class="col-xs-12">
      <nav class="mobile-menu">
        <ul class="mobile-ul">
          <li class="shopping-icon"> <a href="/cart"> <i class="fa fa-shopping-cart"></i> <span class="number-amount"><%= @order.try(:line_items)? @order.try(:line_items).count : 0 %></span> </a> </li>
          <li><a href="/">home</a></li>
          <li><a href="/page/<%= @about_page.first.try(:id)%>">about us</a></li>
          <li><a href="/products">service</a></li>
          <li><a href="/messages"> Contacts</a></li>
        </ul>
      </nav>
    </div>
  </div>
  <!-- end mobile-menu-wrap -->
  <div class="header-color">
    <div class="menu-color">
      <figure class="logo"> <a href="/" title="logo" rel="home"> <img alt="Atropos" src="<%= Spree::Logo.last.avatar.url(:logo)%>" </a>
        <div class=" nav-menu" style="float:right;"> <a href=<%= @org.facebook_url|| '#'%> target="_blank"><img src="/assets/<%= @selected_template.template_no%>/social/f-icon.png"></a> <a href=<%= @org.google_url|| '#'%> target="_blank"><img src="/assets/<%= @selected_template.template_no%>/social/g-icon.png"></a> <a href=<%= @org.twitter_url|| '#'%> target="_blank"><img src="/assets/<%= @selected_template.template_no%>/social/t-icon.png"></a> </div>
      </figure>
    </div>
    <!-- end col-sm-3 -->
    
    <div class="container abc">
      <div class="col-md-12 nav-menu nav-align">
        <div class="col-md-2"></div>
        <div class="col-md-8">
        
       
        
          <center>
            <span class="menu-sep"><a class="active" href="/">Home</a> </span> 
            <span class="menu-sep"><a href="/products">Services</a> </span>

             <% @customize_pages.each_with_index  do |page, index|%>
                <% if index >= 2 %>
                  <% if index == 2 %>
                  <span class="nav-menu pull-right" >
                    <ul class="nav navbar-nav" style="margin-top:0px;">
                      <li class="more-li">
                        <a class="more-color" href="#">
                          More <i class="fa fa-angle-down"></i>
                        </a>
                        <ul class="sub-menu">
                  <% end %>
                       <li class="menu-sep"><a href="/page/<%= page.id %>"><%= page.title%></a></li>
                  <% if index+1 == @customize_pages.length %>
                        </ul>
                      </li>
                    </ul>
                  </span>
                  <% end %>
                <% else %>
                  <span class="menu-sep"><a href="/page/<%= page.id %>"> <%= page.title%></a></span>
                <% end %>
            <% end %>
            <span class="menu-sep"> <a href="/page/<%= @about_page.first.try(:id)%>"> About Us </a></span>
            <span class="menu-sep"><a href="/messages"> Contact Us</a></span>
          </center>
        </div>
        
        <div class="col-md-2"> <span class="nav-menu pull-right" >
          <ul class="nav navbar-nav" style="margin-top:0px;">
            <li><a href="/cart"><i class="fa fa-shopping-cart"></i></a> <span class="number-amount"><%= @order.try(:line_items)? @order.try(:line_items).count : 0 %></span> </a>
            </li>
          </ul>
          </span> </div>
      </div>
    </div>
  </div>

  <!-- end container --> 
  
</header>

<!-- END HEADER --> 

<!-- FIXED HEADER -->

<header class="fixed-header">
  <div class="fixed-menu-cover">
    <div class="container">
      <div class="col-sm-2 col-md-1 col-lg-2">
        <figure class="logo"> <a href="/" title="logo" rel="home"> <img alt="Atropos" src="<%= Spree::Logo.last.avatar.url(:logo)%>" </a> </figure>
      </div>
      <!-- col-sm-3 -->
      
      <div class="col-sm-10 col-md-10 col-lg-10">
        <nav class="nav-menu">
          <ul class="nav navbar-nav">
            <li><a href="/">Home</a></li>
            <li><a href="/products">Services</a></li>
             <li><a href="/page/<%= @about_page.first.try(:id)%>">About Us</a></li>
            <li><a href="/messages"> Contact Us</a> </li>
             <% @customize_pages.each_with_index  do |page, index|%>
                <% if index >= 2 %>
                  <% if index == 2 %>
                   <li>
                     <a href="#" style="border-bottom-width: 2px; border-bottom-style: solid; border-bottom-color: transparent;">More</a>
                       <ul class="sub-menu" style="display: none;">
                  <% end %>
                        <li><a href="/page/<%= page.id %>"><%= page.title%></a></li>
                  <% if index+1 == @customize_pages.length %>
                      </ul>
                    </li>
                  <% end %>
                <% else %>
                <li><a href="/page/<%= page.id %>"><%= page.title%></a> </li>
                <% end %>
            <% end %>
            <li class="pull-right" style="margin-right:35px;" ><a href="/cart"><i class="fa fa-shopping-cart"></i></a> <span class="number-amount"><%= @order.try(:line_items)? @order.try(:line_items).count : 0 %></span> </a>
            </li>
          </ul>
        </nav>
      </div>
    </div>
    <!-- end container --> 
    
  </div>
  <!-- end fixed-header-cover --> 
  
</header>

<!--END FIXED HEADER --> 

